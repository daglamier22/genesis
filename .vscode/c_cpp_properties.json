{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/genesis/include/**",
                "${workspaceFolder}/genesis/src/**",
                "${workspaceFolder}/build/_deps/quill-src/quill/include",
                "${workspaceFolder}/build/_deps/glfw-src/include",
                "${workspaceFolder}/build/_deps/glm-src",
                "${VULKAN_SDK}/include"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_GNU_SOURCE"
            ],
            "intelliSenseMode": "linux-gcc-x64",
            "compilerPath": "/usr/bin/g++",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "forcedInclude": [
                "${workspaceFolder}/build/genesis/CMakeFiles/genesis.dir/cmake_pch.hxx"
            ]
        }
    ],
    "version": 4
}
